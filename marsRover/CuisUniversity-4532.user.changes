	(anInstruction = $b) ifTrue: [^self moveRoverBackwards].
	(anInstruction = $l) ifTrue: [^self rotateRoverLeft].
	(anInstruction = $r) ifTrue: [^self rotateRoverRight].
	self error: self class instruccionNotUnderstoodError
	! !
